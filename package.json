{
  "name": "fastify-errors-properties",
  "version": "1.0.0",
  "description": "Fastify plugin to support http-errors additional properties.",
  "homepage": "https://sw.cowtech.it/fastify-errors-properties",
  "repository": "github:ShogunPanda/fastify-errors-properties",
  "keywords": [
    "fastify",
    "fastify-plugin",
    "http-errors"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/fastify-errors-properties/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "files": [
    "lib",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/*.ts test/*.ts",
    "test": "tap --reporter=spec --coverage-report=html --coverage-report=text --no-browser test/*.test.ts",
    "test:ci": "tap --no-color --reporter=spec --coverage-report=json --coverage-report=text --branches 90 --functions 90 --lines 90 --statements 90 test/*.test.ts",
    "ci": "yarn lint && yarn test:ci",
    "prebuild": "rm -rf lib/* types/* && yarn lint",
    "build": "tsc -p .",
    "prepublishOnly": "yarn ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "ajv": "^6.12.5",
    "fastify-plugin": "^2.3.4",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.4",
    "statuses": "^2.0.0"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^7.10.0",
    "@types/http-errors": "^1.8.0",
    "@types/node": "^14.11.2",
    "@types/statuses": "^2.0.0",
    "@types/tap": "^14.10.1",
    "fastify": "^3.4.1",
    "prettier": "^2.1.2",
    "tap": "^14.10.8",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=12.15.0"
  }
}
