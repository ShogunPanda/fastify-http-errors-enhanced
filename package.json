{
  "name": "fastify-http-errors-enhanced",
  "version": "4.0.3",
  "description": "A error handling plugin for Fastify that uses enhanced HTTP errors.",
  "homepage": "https://sw.cowtech.it/fastify-http-errors-enhanced",
  "repository": "github:ShogunPanda/fastify-http-errors-enhanced",
  "keywords": [
    "fastify",
    "fastify-plugin",
    "http-errors",
    "http-errors-enhanced"
  ],
  "bugs": {
    "url": "https://github.com/ShogunPanda/fastify-http-errors-enhanced/issues"
  },
  "author": "Shogun <shogun@cowtech.it>",
  "license": "ISC",
  "private": false,
  "files": [
    "dist",
    "types",
    "CHANGELOG.md",
    "LICENSE.md",
    "README.md"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "typings": "./types/index.d.ts",
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "swc -s -w -d dist src",
    "prebuild": "rm -rf dist types && npm run typecheck && npm run lint",
    "build": "swc --delete-dir-on-start -d dist src",
    "format": "prettier -w src test",
    "typecheck": "tsc -p . --emitDeclarationOnly",
    "lint": "eslint src test",
    "test": "c8 -c test/config/c8-local.json tap --rcfile=test/config/tap.yml test/*.test.ts",
    "test:ci": "c8 -c test/config/c8-ci.json tap --rcfile=test/config/tap.yml --no-color test/*.test.ts",
    "ci": "npm run build && npm run test:ci",
    "prepublishOnly": "npm run ci",
    "postpublish": "git push origin && git push origin -f --tags"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "fastify-plugin": "^4.2.1",
    "http-errors-enhanced": "^1.0.9"
  },
  "devDependencies": {
    "@cowtech/eslint-config": "^8.7.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.245",
    "@types/node": "^18.7.14",
    "@types/tap": "^15.0.7",
    "ajv-formats": "^2.1.1",
    "c8": "^7.12.0",
    "chokidar": "^3.5.3",
    "fastify": "^4.5.3",
    "prettier": "^2.7.1",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=14.15.0"
  }
}
